#! /bin/bash

mv mojave/ ../
mv runDynamicWallpaper ../
exe=$PWD
mkdir ~/.config
mkdir ~/.config/autostart

echo "[Desktop Entry]
Encoding=UTF-8
Version=0.9.4
Type=Application
Name=Dynamic Wallpaper
Comment=Mojave Dynamic Wallpaper
Exec=$exe
OnlyShowIn=XFCE;
StartupNotify=false
Terminal=false
Hidden=false" >> 'Dynamic Wallpaper.desktop'
mv 'Dynamic Wallpaper.desktop' ~/.config/autostart

cd ..
rm -rf Xubuntu-Dynamic-Wallpaper

